using System;

namespace sum
{
    class Program
    {
        static void Main(string[] args)
      {
            Console.WriteLine(sum1(2,3));
            Console.WriteLine(sum1(3,3));
         
        }
        public static int sum1(int x, int y)
        {
            return x == y ? (x+y)*3 : x + y;
            //  (condition) ? {code for YES} : {code for NO}
            Console.ReadLine();
        }
    }
}
